   "source": [
    "def task5_bubble_chart(ignore_time=False):\n",
    "    import pandas as pd\n",
    "    import numpy as np\n",
    "    import plotly.graph_objects as go\n",
    "    from datetime import datetime\n",
    "    import pytz\n",
    "\n",
    "    # ----------------------------\n",
    "    # Time Restriction: 5 PM – 7 PM IST\n",
    "    # ----------------------------\n",
    "    if not ignore_time:\n",
    "        ist = pytz.timezone(\"Asia/Kolkata\")\n",
    "        current_hour = datetime.now(ist).hour\n",
    "        if not (17 <= current_hour < 19):\n",
    "            print(\"Chart visible only between 5 PM and 7 PM IST.\")\n",
    "            return\n",
    "\n",
    "    # ----------------------------\n",
    "    # Load Dataset\n",
    "    # ----------------------------\n",
    "    file_path = r\"C:\\Users\\user\\OneDrive\\Desktop\\Null Class\\Google Playstore Apps.csv\"\n",
    "    df = pd.read_csv(file_path)\n",
    "\n",
    "    # ---------- CLEAN INSTALLS ----------\n",
    "    if \"Installs\" in df.columns:\n",
    "        df[\"Installs\"] = df[\"Installs\"].astype(str).str.replace(\"+\", \"\", regex=False).str.replace(\",\", \"\", regex=False)\n",
    "        df[\"Installs\"] = pd.to_numeric(df[\"Installs\"], errors=\"coerce\")\n",
    "\n",
    "    # ---------- CLEAN REVIEWS & RATING ----------\n",
    "    if \"Reviews\" in df.columns:\n",
    "        df[\"Reviews\"] = pd.to_numeric(df[\"Reviews\"], errors=\"coerce\")\n",
    "    if \"Rating\" in df.columns:\n",
    "        df[\"Rating\"] = pd.to_numeric(df[\"Rating\"], errors=\"coerce\")\n",
    "\n",
    "    # ---------- FIND SIZE COLUMN ----------\n",
    "    size_column = None\n",
    "    for col in df.columns:\n",
    "        if \"size\" in col.lower():\n",
    "            size_column = col\n",
    "            break\n",
    "\n",
    "    if size_column is None:\n",
    "        print(\"No Size column found in dataset.\")\n",
    "        return\n",
    "\n",
    "    # Convert Size to MB\n",
    "    def convert_size(value):\n",
    "        if isinstance(value, str):\n",
    "            if \"M\" in value:\n",
    "                return float(value.replace(\"M\", \"\"))\n",
    "            elif \"k\" in value:\n",
    "                return float(value.replace(\"k\", \"\")) / 1024\n",
    "        return np.nan\n",
    "\n",
    "    df[\"Size_MB\"] = df[size_column].apply(convert_size)\n",
    "    df.dropna(subset=[\"Rating\", \"Reviews\", \"Installs\", \"Size_MB\"], inplace=True)\n",
    "\n",
    "    # ---------- FILTERING ----------\n",
    "    required_categories = [\n",
    "        \"GAME\", \"BEAUTY\", \"BUSINESS\", \"COMICS\",\n",
    "        \"COMMUNICATION\", \"DATING\", \"ENTERTAINMENT\",\n",
    "        \"SOCIAL\", \"EVENTS\"\n",
    "    ]\n",
    "\n",
    "    df = df[df[\"Rating\"] > 3.5]\n",
    "    df = df[df[\"Reviews\"] > 500]\n",
    "    df = df[df[\"Installs\"] > 50000]\n",
    "    df = df[~df[\"App\"].str.contains(\"S\", case=False, na=False)]\n",
    "    df = df[df[\"Category\"].isin(required_categories)]\n",
    "\n",
    "    if \"Avg_Sentiment_Subjectivity\" in df.columns:\n",
    "        df = df[df[\"Avg_Sentiment_Subjectivity\"] > 0.5]\n",
    "\n",
    "    if df.empty:\n",
    "        print(\"No data left after applying filters.\")\n",
    "        return\n",
    "\n",
    "    # ---------- TRANSLATION ----------\n",
    "    translation = {\n",
    "        \"BEAUTY\": \"सौंदर्य\",\n",
    "        \"BUSINESS\": \"வணிகம்\",\n",
    "        \"DATING\": \"Partnersuche\"\n",
    "    }\n",
    "    df[\"Category_Display\"] = df[\"Category\"].replace(translation)\n",
    "\n",
    "    # ---------- COLOR MAP ----------\n",
    "    color_map = {\n",
    "        \"GAME\": \"pink\",\n",
    "        \"सौंदर्य\": \"#2ecc71\",\n",
    "        \"வணிகம்\": \"#3498db\",\n",
    "        \"Partnersuche\": \"#9b59b6\",\n",
    "        \"COMICS\": \"#f39c12\",\n",
    "        \"COMMUNICATION\": \"#e74c3c\",\n",
    "        \"ENTERTAINMENT\": \"#8e44ad\",\n",
    "        \"SOCIAL\": \"#16a085\",\n",
    "        \"EVENTS\": \"#7f8c8d\"\n",
    "    }\n",
    "\n",
    "    # ---------- CREATE PLOT ----------\n",
    "    fig = go.Figure()\n",
    "\n",
    "    for category in df[\"Category_Display\"].unique():\n",
    "        subset = df[df[\"Category_Display\"] == category]\n",
    "\n",
    "        fig.add_trace(go.Scatter(\n",
    "            x=subset[\"Size_MB\"],\n",
    "            y=subset[\"Rating\"],\n",
    "            mode=\"markers\",\n",
    "            name=category,\n",
    "            text=subset[\"App\"],\n",
    "            marker=dict(\n",
    "                size=np.sqrt(subset[\"Installs\"]) / 40,\n",
    "                color=color_map.get(category, \"white\"),\n",
    "                opacity=0.8,\n",
    "                line=dict(width=1, color=\"white\")\n",
    "            ),\n",
    "            hovertemplate=\"<b>%{text}</b><br>Size: %{x} MB<br>Rating: %{y}<extra></extra>\"\n",
    "        ))\n",
    "\n",
    "    fig.add_hline(y=4.0, line_dash=\"dash\", line_color=\"yellow\")\n",
    "\n",
    "    fig.update_layout(\n",
    "        title=\"App Size vs Rating (Bubble Size = Installs)\",\n",
    "        xaxis_title=\"App Size (MB)\",\n",
    "        yaxis_title=\"Average Rating\",\n",
    "        template=\"plotly_dark\",\n",
    "        height=650\n",
    "    )\n",
    "\n",
    "    fig.show()\n",
    "\n",
    "task5_bubble_chart()"
   ]
